var A = [5, 87, 3, 6, 9, 10, 2, 1]
var n = 8

var i as Int
println("Unsorted Array")
for i = 0; i < 8; i += 1; {
	print(A[i] + " ")
}
println("\n")

for i = 0; i < n - 1; i += 1; {
	for var j = 0; j < n - 1; j += 1; {
		if A[j] > A[j+1] {
			var temp = A[j]
			A[j] = A[j+1]
			A[j+1] = temp
		}
	}
}

println("Sorted Array")
for i = 0; i < 8; i += 1; {
	print(A[i] + " ")
}
println()
